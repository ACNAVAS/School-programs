//accumalatorV1.c
#include<stdio.h>
#include<math.h>

int main()
{
	int accum = 0;
	int num;
	
	printf("Enter a number: ");
	scanf("%d", &num);
	
	accum = accum + num;

	printf("Enter a number: ");
	scanf("%d", &num);
	
	accum = accum + num;
	
	printf("Enter a number: ");
	scanf("%d", &num);
	
	accum = accum + num;
	
	printf("%d\n",accum);
	system("PAUSE");
	return 0;
}
