#include<stdio.h>
#include<math.h>
#include<windows.h>
void edad(int age)
{
	printf("How old are you? ");
	scanf("%d", &age);
	printf("You are %d years old!\n", age);
}

int main()
{
	int name;
	int age; 
	
	printf("Name: ");
	scanf("%d", &name);
	system("CLS");
	
	void edad(age);
	
	system("PAUSE");
	return 0;
}
