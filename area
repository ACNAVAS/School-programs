//Author:Cesar
//Area
#include<stdio.h>
int main()
{
	int width, length;
	
	printf("Enter the width: ");
	scanf("%d", &width);
	printf("Enter the Length ");
	scanf("%d", &length);
		
	if ( width > 0 && length > 0);
		printf(" The area of the rectangle is %d\n", width * length);
	else; 
		printf("Invalid width and/or length!\n");
	system("PAUSE");
	return 0;
}
