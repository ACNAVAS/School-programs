#include<stdio.h>


int main()
{
	int n;
	int i;
	
	printf("How many numbers do you want to enter? ");
	scanf("%d", &n);
	
	int a[n];
	
	for (i = 0; i < n; i++)
	{
		printf("Enter a number: ");
		scanf("%d\n",&a[i]);
	}
	for (i =0; i < n; i++)
	{
		printf(")	
	}
/*	
	
	for (i =0; i < n; i++)
	{
		
	}	
	*/
	system("PAUSE");
	return 0;
}
